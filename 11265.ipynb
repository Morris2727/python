    "def dec_to_bin(dec):\n",
    "    if(dec == 0): return 0\n",
    "    \n",
    "    out = ''\n",
    "    while(dec):\n",
    "        out = str(dec & 1) + out\n",
    "        dec >>= 1\n",
    "    \n",
    "    return out"
   ]
